#!/usr/bin/expect -f
set timeout -1
catch {unset env(CLAUDECODE)}

cd $env(HOME)/qz/toocan-app
spawn claude

sleep 4

# Type & prefix + prompt
send "& "
sleep 0.5
send "test 6.0.0"
sleep 1

# Use AppleScript to send a real Return keystroke to the frontmost terminal
exec osascript -e {tell application "System Events" to key code 36}

sleep 3

interact
